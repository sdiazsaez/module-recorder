            "aliases": {
                "ModuleRegister": "Larangular\\ModuleRecorder\\Facades\\ModuleRegister",
                "ProviderRegister": "Larangular\\ModuleRecorder\\Facades\\ProviderRegister",
                "ModuleRequest": "Larangular\\ModuleRecorder\\Facades\\ModuleRequest"
            }
        }
    }
