    "require": {
        "php": ">=7.1",
        "illuminate/support": "*",
        "larangular/support": "^0.1.0|^0.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\ModuleRecorder\\": "./src"
        }
